---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero.svelte";
import {HolidazeGateWay} from "../gateway/HolidazeGateway"

const holidazeGateWay = new HolidazeGateWay()
let data
try {
  // Assuming getVenues expects an object; adjust accordingly if needed
  data = await holidazeGateWay.getVenues({ query: '' });
  console.log(data);
} catch (error) {
  console.error('Failed to fetch data:', error);
  data = {}; // Ensure data has a fallback value
}
---

<Layout title="Welcome to Astro.">
  <main>
    <Hero data={data} />
  </main>
</Layout>

<style></style>
