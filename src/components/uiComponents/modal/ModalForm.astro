---
import { Modal } from "accessible-astro-components";
import BookinForm from "../../forms/bookingForm/BookinForm.svelte";

const { bookings, max, id, price } = Astro.props;
---

<Modal triggerId="modal1-trigger" title="">
  <BookinForm
    client:load
    bookings={bookings ?? []}
    max={max}
    id={id}
    price={price}
  />
</Modal>
